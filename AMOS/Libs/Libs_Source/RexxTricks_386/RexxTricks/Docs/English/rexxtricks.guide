     list.0 = 4
     list.1 = 'Hello'
     list.2 = 'Good morning'
     list.3 = 'Bye bye'
     list.4 = 'Nice to see you'
     
     IF STEMINSERT('list',3,2,'New') THEN DO
        SAY 'New number of elements:' list.0
     
        DO i = 1 TO liste.0
           SAY liste.i
        END
     END
     ELSE
        SAY 'Error inserting elements!'

